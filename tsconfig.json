{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
    "sourceMap": true,
    "removeComments": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "rootDirs": [
      "src",
    ],
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "*"
      ]
    },
  },
  "include": [
    "src",
  ],
  "exclude": []
}